<interface>
  <object id="window" class="GtkWindow">
    <property name="visible">True</property>
    <property name="title">Liwiro</property>
    <property name="border-width">10</property>
    <child>
      <object id="grid" class="GtkGrid">
        <property name="visible">True</property>
		<child>
          <object class="GtkEventBox" id="eventboxstop">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="events">GDK_BUTTON_PRESS_MASK</property>
            <child>
              <object class="GtkImage" id="imagestop">
                <property name="visible">True</property>
                <property name="can_focus">False</property>			
                <property name="icon_size">3</property>
                <property name="stock">gtk-stop</property>
              </object>
            </child>
            <accelerator key="KP_5" signal="composited-changed"/>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="top_attach">1</property>
          </packing>
        </child>
        <child>
          <object id="quit" class="GtkButton">			  
			<style>
				<class name="destructive-action"/>
			</style>
            <property name="visible">True</property>
            <property name="label">Q_uit</property>			
			<property name="use_underline">True</property>
          </object>
          <packing>
            <property name="left-attach">4</property>
            <property name="top-attach">2</property>
            <property name="width">1</property>
          </packing>
        </child>
		<child>
          <object id="information" class="GtkButton">
			<style>
				<class name="suggested-action"/>
			</style>
            <property name="visible">True</property>
            <property name="label">_Information</property>			
			<property name="use_underline">True</property>			
            <accelerator key="KP_1" signal="clicked"/>
          </object>
          <packing>
            <property name="left-attach">3</property>
            <property name="top-attach">0</property>
            <property name="width">2</property>
          </packing>
        </child>
		<child>
          <object class="GtkEventBox" id="eventboxup">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="events">GDK_BUTTON_PRESS_MASK</property>
            <child>
              <object class="GtkImage" id="imageup">
                <property name="visible">True</property>
                <property name="can_focus">False</property>				
                <property name="icon_size">3</property>
                <property name="stock">gtk-go-up</property>
              </object>
            </child>
            <accelerator key="KP_8" signal="composited-changed"/>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="top_attach">0</property>
          </packing>
        </child>
		<child>
          <object class="GtkEventBox" id="eventboxdown">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="events">GDK_BUTTON_PRESS_MASK</property>
            <child>
              <object class="GtkImage" id="imagedown">
                <property name="visible">True</property>
                <property name="can_focus">False</property>			
                <property name="icon_size">3</property>
                <property name="stock">gtk-go-down</property>
              </object>
            </child>
            <accelerator key="KP_2" signal="composited-changed"/>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="top_attach">2</property>
          </packing>
        </child>
		<child>
          <object class="GtkEventBox" id="eventboxleft">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="events">GDK_BUTTON_PRESS_MASK</property>
            <child>
              <object class="GtkImage" id="imageleft">
                <property name="visible">True</property>
                <property name="can_focus">False</property>			
                <property name="icon_size">3</property>
                <property name="stock">gtk-go-back</property>
              </object>
            </child>
            <accelerator key="KP_4" signal="composited-changed"/>
          </object>
          <packing>
            <property name="left_attach">0</property>
            <property name="top_attach">1</property>
          </packing>
        </child>
		<child>
          <object class="GtkEventBox" id="eventboxright">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="events">GDK_BUTTON_PRESS_MASK</property>
            <property name="margin_right">30</property>
            <child>
              <object class="GtkImage" id="imageright">
                <property name="visible">True</property>
                <property name="can_focus">False</property>			
                <property name="icon_size">3</property>
                <property name="stock">gtk-go-forward</property>
              </object>
            </child>
            <accelerator key="KP_6" signal="composited-changed"/>
          </object>
          <packing>
            <property name="left_attach">2</property>
            <property name="top_attach">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkDrawingArea" id="drawingarea1">
            <property name="width_request">200</property>
            <property name="height_request">200</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="margin_left">57</property>
            <property name="margin_right">57</property>
            <property name="margin_top">20</property>
            <property name="margin_bottom">20</property>
			<property name="events">GDK_BUTTON_PRESS_MASK | GDK_POINTER_MOTION_MASK</property>
          </object>
          <packing>
            <property name="left_attach">0</property>
            <property name="top_attach">7</property>
            <property name="width">5</property>
          </packing>
        </child>
      </object>
      <packing>
      </packing>
    </child>
  </object>
</interface>